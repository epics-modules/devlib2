record(longin, "in_u8") {
    field(DTYP, "Explore Read8")
    field(INP , "@0:1.0 bar=0 offset=4")
    field(PINI, "YES")
    field(TPRO, "$(TPRO=0)")
}

record(longin, "in_u16") {
    field(DTYP, "Explore Read16 MSB")
    field(INP , "@0:1.0 bar=0 offset=4")
    field(PINI, "YES")
    field(TPRO, "$(TPRO=0)")
}

