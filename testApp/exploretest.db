record(waveform, "in_vec_u16") {
    field(DTYP, "Explore Read16 MSB")
    field(INP , "@0:1.0 bar=0 offset=4")
    field(FTVL, "USHORT")
    field(NELM, "128")
    field(PINI, "YES")
    field(TPRO, "$(TPRO=0)")
}



